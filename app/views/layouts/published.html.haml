!!!
%html{ lang: "en" }
  %head
    %title PlusPanda Sample Staging Page
    = stylesheet_link_tag "staging"

    - if Rails::env == "development"
      = javascript_include_tag "jquery/jquery"
    - else
      = javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js"

  %body
    #wrapper
      #content_wrapper
        = yield

    = raw embed_code(@user.apikey)
